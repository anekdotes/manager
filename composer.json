  "description": "Provides an easier way to upload images with configurations",
  "keywords": [
    "manager",
    "image",
    "file",
    "upload"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Gosselin",
      "email": "mathieu.gosselin@anekdotes.com",
      "homepage": "http://www.anekdotes.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "imagine/imagine": "*",
    "anekdotes/file": "*"
  },
  "require-dev": {
